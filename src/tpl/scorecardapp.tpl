<div id="header" class="section">
	<h1 id="tournament-name"></h1>
	<h2 id="tournament-host-course"></h2>
	<h3 id="tournament-dates"></h3>
</div>
<div id="summary" class="section">
	<h1>TOURNAMENT SUMMARY</h1>
	<table>
		<thead>
			<th>Overall Position</th>
			<th>Overall to Par</th>
			<th>Starting Hole</th>
			<th>Current Round</th>
			<th>Current Round to Par</th>
			<th class="last">Current Round Thru</th>
		</thead>
		<tbody>
			<tr>
				<td id="overall-position"></td>
				<td id="overall-to-par"></td>
				<td id="starting-hole"></td>
				<td id="current-round"></td>
				<td id="current-round-to-par"></td>
				<td id="current-round-thru" class="last"></td>
			</tr>
		</tbody>
	</table>
</div>
